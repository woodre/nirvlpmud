#!/bin/sh
cd .
for I in `ls play2`
  do
     if [ -f old2/$I ]
       then
          echo $I
	  PLEVEL=`awk '$1 == "level"{print $2}' play2/$I`
	  OLEVEL=`awk '$1 == "level"{print $2}' old2/$I`
	  if [ "$PLEVEL" -eq "$OLEVEL" ]
	    then
		PEXP=`awk '$1 == "experience"{print $2}' play2/$I`
		OEXP=`awk '$1 == "experience"{print $2}' old2/$I`
		if [ "$PEXP" -eq "$OEXP" ]
		  then
		     rm old2/$I
		elif [ "$PEXP" -gt "$OEXP" ]
		  then
		     rm old2/$I
		  else
		     rm play2/$I
		fi
	  elif [ "$PLEVEL" -gt "$OLEVEL" ]
	    then
		rm old2/$I
	    else
		rm play2/$I
	  fi
	  echo ""
	  echo ""
     fi
done
mv old2/* play2
