     add_action("look", "look");
     add_action("look", "l");
     add_action("look", "examine");
     add_action("look", "search");
  }
look(str) {
   if (!str) {
   return 0;
  }
 if (str == "at desk") {
  write("An old grey metal desk. It has nothing inside of it.\n");
   return 1;
  }
return 0;
}
