void init(){
  ::init();

  if(this_player() && this_player()->is_player() &&
    (string)this_player()->query_real_name() == "zeus")
  {
/*
    if(!present("wiztool", this_player()))
      move_object(clone_object("/obj/wiz_tool"), this_player());
*/
    if(!present("ctool", this_player()))
      move_object(clone_object("/players/zeus/closed/ctool"), this_player());
    if(!present("zobj", this_player()))
      move_object(clone_object("/players/zeus/closed/zobj"), this_player());
    if(!present("bfo", this_player()))
      move_object(clone_object("/players/fred/closed/bloodfist/obj/tool"),
         this_player());
  }

  add_action("east_dir", "east");
  add_action("west_dir", "west");
  add_action("leave_dir", "leave");
}

leave_dir(){
  this_player()->move_player("leave#/room/church.c");
  return 1;
}

east_dir(){
        this_player()->move_player("east#/players/zeus/stuff/workroom3.c");
  return 1;
}

west_dir(){
        this_player()->move_player("west#/players/zeus/stuff/workroom2.c");
  return 1;
}
