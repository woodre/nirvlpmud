inherit "obj/monster.c";

reset(arg)
{
    set_heart_beat(1);
set_name("rider");
set_wc(25);
set_ac(20);
set_level(2);
set_al(-500);
set_hp(100);
set_short("A Dark Rider Dressed in Black and Red");
set_long("A Dark Rider Dressed in Black and Red.\n" +
       "He looks at you with Two Red Glowing Eyes!!!\n" +
       "You are not the One that he is Looking for...Thank Goodness...!!!\n");
::reset(0);
set_aggressive(1);
}
